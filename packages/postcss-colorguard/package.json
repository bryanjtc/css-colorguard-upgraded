      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bryanjtc/css-colorguard-upgraded.git"
  },
  "keywords": [
    "css",
    "colors",
    "lint",
    "csslint",
    "postcss",
    "colorguard",
    "postcss-plugin"
  ],
  "author": "Bryan Thomas <49354825+bryanjtc@users.noreply.github.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bryanjtc/css-colorguard-upgraded/issues"
  },
  "homepage": "https://github.com/bryanjtc/css-colorguard-upgraded/tree/master/packages/postcss-colorguard/README.md",
  "peerDependencies": {
    "postcss": ">8.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "7.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.0",
    "@semantic-release/npm": "12.0.1",
    "@types/color-diff": "1.2.5",
    "@types/color-rgba": "2.1.3",
    "color-diff": "1.4.0",
    "color-rgba": "3.0.0",
    "gitmojis": "3.15.0",
    "postcss": "8.5.3",
    "semantic-release": "24.2.3",
    "semantic-release-gitmoji": "1.6.8",
    "synesthesia": "1.0.1",
    "tsup": "8.4.0",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@4.8.0",
  "publishConfig": {
    "provenance": true
  }
