{
  "name": "colorguard-processor",
  "version": "2.0.99",
  "description": "PostCSS processor to keep a watchful eye on your css colors",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn run build",
    "build": "tsup",
    "test": "tape test/*.ts | tap-spec"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bryanjtc/css-colorguard-upgraded.git"
  },
  "keywords": [
    "css",
    "colors",
    "lint",
    "csslint",
    "postcss",
    "colorguard",
    "postcss-processor"
  ],
  "author": "Bryan Thomas <49354825+bryanjtc@users.noreply.github.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bryanjtc/css-colorguard-upgraded/issues"
  },
  "homepage": "https://github.com/bryanjtc/css-colorguard-upgraded/tree/master/packages/colorguard-processor/README.md",
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.0",
    "@semantic-release/npm": "12.0.1",
    "@types/node": "22.10.2",
    "@types/postcss-reporter": "7.0.5",
    "@types/tape": "5.7.0",
    "@types/text-table": "0.2.5",
    "colorette": "2.0.20",
    "gitmojis": "3.14.0",
    "log-symbols": "7.0.0",
    "plur": "5.1.0",
    "postcss": "8.4.49",
    "postcss-colorguard": "2.0.23",
    "postcss-reporter": "7.1.0",
    "semantic-release": "24.2.0",
    "semantic-release-gitmoji": "1.6.8",
    "tap-spec": "5.0.0",
    "tape": "5.9.0",
    "text-table": "0.2.0",
    "tsup": "8.3.5",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@4.5.3",
  "publishConfig": {
    "provenance": true
  }
}
